<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>

  <% if( locals.loggedin == 1) { %>
  <div id="tools">
    <a title="Click to go to the account management view" href="/account/">Welcome <%- locals.accountData.account_firstname %>   </a>
    <a title="Click to log out" href="/account/logout">log-out</a>
  </div>
  <% } else {%>
  <div id="tools">
    <a title="Click to log in" href="/account/login">My Account</a>
  </div>
  <% } %>
</header>
