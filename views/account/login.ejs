<%# code to add the main heading element and giving it a value the same as the
title variable %> <% if (title) { %>
<h1><%- title %></h1>
<% } else {res.redirect('/')}%>

<%- messages() %>

<form>
  <label for="email">Email:</label>
  <input type="email" id="email" name="account_email" />
  <label for="password">Password:</label>
  <input type="password" id="password" name="account_password" />
  <button>Login</button>
  <p>No account? <a href="/account/register">Sign-up</a></p>
</form>
